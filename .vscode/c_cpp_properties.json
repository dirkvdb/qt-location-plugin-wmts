{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceRoot}",
                "/usr/local/opt/qt/include",
                "/usr/local/include",
                "/usr/local/include/c++/v1"
            ],
            "intelliSenseMode": "clang-x64",
            "macFrameworkPath": [
                "/usr/local/opt/qt/lib",
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compileCommands": "${workspaceRoot}/build/compile_commands.json"
        },
        {
            "name": "Linux",
            "includePath": [
                "/usr/include",
                "${workspaceRoot}"
            ],
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "/usr/include",
                    "${workspaceRoot}"
                ]
            }
        },
        {
            "name": "Win32",
            "includePath": [
                "c:/Program Files (x86)/Microsoft Visual Studio 14.0/VC/include",
                "${workspaceRoot}"
            ],
            "intelliSenseMode": "msvc-x64",
            "browse": {
                "path": [
                    "c:/Program Files (x86)/Microsoft Visual Studio 14.0/VC/include",
                    "${workspaceRoot}"
                ]
            }
        }
    ],
    "clang_format": {
        "style": "file",
        "fallback-style": "None",
        "sort-includes": true
    },
    "version": 3
}